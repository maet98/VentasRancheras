{
  "name": "api2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "scripts": {
    "start": "npm run prod",
    "build": "npm-run-all clean transpile",
    "server": "node ./dist-server/index",
    "dev": "NODE_ENV=development npm-run-all build server",
    "prod": "NODE_ENV=production npm-run-all build server",
    "transpile": "babel ./server --out-dir dist-server",
    "clean": "rimraf dist-server",
    "watch:dev": "nodemon"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "server/*",
      "public/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*.spec.js"
    ],
  "author": "Miguel Estevez",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "body-parser": "^1.19.0",
    "config": "^3.3.0",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "intuit-oauth": "^2.0.2",
    "morgan": "^1.9.1",
    "node-quickbooks": "^2.0.33",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "rimraf": "^3.0.2",
    "config": "^3.3.0",
    "express": "^4.17.1",
    "node-quickbooks": "^2.0.33"
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  }
}
